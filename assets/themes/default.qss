/* SECTION_Center */
#ListWidget_Saves
{
    border: none;
    background: #222;
}
#ListWidget_Saves QPushButton #Button_DeleteSave
{
    border: none;
    height: 20px;
    width: 20px;
    background: transparent;
    background-image: url(':imgs/bDel0');
}
#ListWidget_Saves QPushButton #Button_DeleteSave:hover
{
    border: none;
    height: 20px;
    width: 20px;
    background-image: url(':imgs/bDel1');
}
#ListWidget_Saves QLabel
{
    color: white;
}
#ListWidget_Saves QPushButton QLabel[Title="1"]/*CF HTML*/
{
    font-family: "Determination Mono";
    font-size: 18pt;
    font-weight: bold;
}
#ListWidget_Saves QPushButton QLabel[Title="2"]/*CF HTML*/
{
    font-family: "Determination Mono";
    font-size: 13pt;
    font-weight: bold;
}
#ListWidget_Saves QPushButton
{
    background: black;
    border: 5px solid #e61;
}
#ListWidget_Saves QPushButton:hover
{
    background: black;
    border: 5px solid #ff1;
}
#ListWidget_Saves QPushButton:checked
{
    background: black;
    border: 5px solid #fff;
}
