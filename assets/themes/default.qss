#Dialog_Settings
{
    background: #111;
    border: 3px solid white;
}
#Dialog_Settings QGroupBox::title
{
    background: white;
    color: black;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 3px;
    padding-bottom: 3px;
    font-weight: bold;
}
#Dialog_Settings #Label_Settings_Header
{
    font-family: "Determination Mono";
    color: lightgrey;
    font-size: 10pt;
}
#Dialog_Settings QComboBox
{
    border: none;
    background-color: black;
    color: white;
    padding: 1px;
    padding-left: 4px;
    height: 15px;
}
#Dialog_Settings QComboBox::drop-down
{
    border: 3px solid black;
    background-color: #333;;
}
#Dialog_Settings QComboBox::drop-down:hover
{
    background-color: #373;;
}
#Dialog_Settings QComboBox QAbstractItemView
{
    color: white;
    border: none;
    outline: none;
    background-color: black;
    selection-background-color: #333;
}
#Dialog_Settings QCheckBox
{
    border: none;
}
#Dialog_Settings QCheckBox::indicator:checked
{
    background: #263;
}
#Dialog_Settings QCheckBox::indicator:checked:hover
{
    background: #283;
}
#Dialog_Settings QCheckBox::indicator:unchecked
{
    background: #623;
}
#Dialog_Settings QCheckBox::indicator:unchecked:hover
{
    background: #823;
}
